func f() (integer, error) { return 4, nil }
x := f()
if x == 4 {
	print("OK")
}

methodik anError struct {} {
	func (a) Error() string { return "anError" }
}

func g() (integer, error) { return 5, anError{} }

x, _ := g()
x, err := g()
if err == nil {
	panic("g should return an error")
}

y, err := f()
z, _ := f()
if x == y && y == z && err == nil {
	print("OK")
}
