x := []string{"zero", "one", "two"}

indexOK := false

if x[0] == "zero" && x[1] == "one" && x[2] == "two" {
	indexOK = true
}

total := 0
sawZero, sawOne, sawTwo := false, false, false
for i, s := range x {
	total++
	if i == 0 && s == "zero" {
		sawZero = true
	}
	if i == 1 && s == "one" {
		sawOne = true
	}
	if i == 2 && s == "two" {
		sawTwo = true
	}
}

totalI := 0
for i := range x {
	totalI += i
}

if indexOK && total == len(x) && totalI == 0+1+2 && sawZero && sawOne && sawTwo {
	print("OK")
}
